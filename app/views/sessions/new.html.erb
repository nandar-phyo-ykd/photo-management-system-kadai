<h1>ログイン</h1>
<%= render partial: 'shared/error_page', locals: { obj: @user } %>
<%= form_with(model: @user, url: login_path, local: true) do |f| %>
    <%= f.label :user_id %><%= f.text_field :user_id %><br/>
    <%= f.label :password %><%= f.password_field :password %><br/>
    <%= f.submit "ログインする" %>
<% end %>
